{% for component in components %}
    {% if component.type  == 'text' %}
		{% include 'IkimeaPageBundle:Components:text.html.twig' with { 'component': component } %}
    {% endif %}

    {% if component.type  == 'flase' %}
		{% include 'IkimeaPageBundle:Components:flash.html.twig' with { 'component': component } %}
    {% endif %}
{% endfor %}

